    "core-js": "^3.4.3",
    "express": "^4.17.1",
    "flexboxgrid": "^6.3.1",
    "fs": "0.0.1-security",
    "vue": "^2.6.10",
    "vue-class-component": "^7.0.2",
    "vue-property-decorator": "^8.3.0",
    "vue-router": "^3.1.3",
    "vuex": "^3.1.2",
    "winston": "^3.2.1"
  },
