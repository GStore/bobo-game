{
  "name": "bobo-game",
  "version": "1.0.0",
  "description": "A game for my son to learn the alphabet",
  "main": "server/index.js",
  "scripts": {
    "server:test": "echo \"Error: no test specified\" && exit 1",
    "server:lint": "eslint -c ./server/.eslintrc.js \"./server/src/**/*.{js,ts,tsx}\" --fix",
    "server:build": "webpack-cli --config server/webpack.config.js",
    "server:api:build": "cross-env BUILD=api webpack-cli --config server/webpack.config.js",
    "server:serve": "npm run server:build && nodemon -r dotenv/config ./dist/server.js",
    "server:start": "node -r dotenv/config ./dist/server.js",
    "storybook": "start-storybook -p 6006",
    "build:storybook": "build-storybook",
    "vue:serve": "vue-cli-service serve",
    "vue:build": "vue-cli-service build",
    "vue:test:unit": "vue-cli-service test:unit",
    "vue:lint": "vue-cli-service lint --fix -c ./ui/.eslintrc.js ui/**/*.ts ui/**/*.js",
    "clean": "rm -rf dist",
    "build": "npm run clean && concurrently \"npm run server:lint\" \"npm run vue:lint\" && concurrently \"npm run server:build\" \"npm run vue:build\"",
    "serve": "cross-env BUILD=devserver npm run server:api:build && cross-env BUILD=devserver npm run vue:serve",
    "start": "npm run build && npm run server:start"
  },
  "author": "StoGun",
  "license": "ISC",
  "dependencies": {
    "adm-zip": "^0.4.13",
    "axios": "^0.19.0",
    "body-parser": "^1.19.0",
